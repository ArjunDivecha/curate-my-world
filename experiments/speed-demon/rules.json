{
  "global": {
    "blockPathTokens": [
      "calendar",
      "whatson", 
      "what's-on",
      "listing",
      "schedule",
      "all-events",
      "events/?$",
      "events$"
    ],
    "requireSignals": {
      "minDateSignals": 0,
      "preferredWords": ["tickets", "buy tickets", "showtimes", "rsvp", "register"]
    }
  },
  "domains": [
    {
      "domain": "thegreekberkeley.com",
      "allowPaths": [
        "^/event/[a-z0-9-]+(\\d{4}-\\d{2}-\\d{2})?$",
        "^/events/[a-z0-9-]+$", 
        "^/tickets/[a-z0-9-]+$"
      ],
      "blockPaths": [
        "^/events/?$",
        "^/calendar/?$",
        "^/event-listing/?$",
        "^/events\\?.*$"
      ]
    },
    {
      "domain": "roxie.com",
      "allowPaths": ["^/film/[a-z0-9-]+$", "^/series/[a-z0-9-]+$"],
      "blockPaths": ["^/$", "^/calendar/?$", "^/films/?$"]
    },
    {
      "domain": "www.sfsymphony.org",
      "allowPaths": [
        "^/buy-tickets/[a-z0-9-]+",
        "^/concerts/[a-z0-9-]+",
        "^/events/[a-z0-9-]+",
        "^/performances/"
      ],
      "blockPaths": ["^/calendar/?$", "^/concerts/?$", "^/season/?$"]
    },
    {
      "domain": "sfjazz.org",
      "allowPaths": [
        "^/events/[a-z0-9-]+",
        "^/shows/[a-z0-9-]+",
        "^/concerts/[a-z0-9-]+",
        "^/tickets/"
      ],
      "blockPaths": ["^/calendar/?$", "^/events/?$", "^/shows/?$"]
    },
    {
      "domain": "www.sfmoma.org",
      "allowPaths": [
        "^/exhibitions/[a-z0-9-]+",
        "^/events/[a-z0-9-]+", 
        "^/programs/[a-z0-9-]+",
        "^/calendar/[a-z0-9-]+"
      ],
      "blockPaths": ["^/calendar/?$", "^/events/?$", "^/exhibitions/?$"]
    },
    {
      "domain": "www.berkeleyrep.org",
      "allowPaths": [
        "^/shows/[a-z0-9-]+",
        "^/tickets/[a-z0-9-]+",
        "^/productions/[a-z0-9-]+",
        "^/events/[a-z0-9-]+"
      ],
      "blockPaths": ["^/season/?$", "^/shows/?$", "^/calendar/?$"]
    },
    {
      "domain": "eventbrite.com",
      "allowPaths": [
        "^/e/[a-z0-9-]+",
        "^/event/[a-z0-9-]+",
        "^/tickets/"
      ],
      "blockPaths": ["^/d/", "^/discover/", "^/search/"]
    },
    {
      "domain": "www.eventbrite.com", 
      "allowPaths": [
        "^/e/[a-z0-9-]+",
        "^/event/[a-z0-9-]+",
        "^/tickets/"
      ],
      "blockPaths": ["^/d/", "^/discover/", "^/search/"]
    },
    {
      "domain": "meetup.com",
      "allowPaths": [
        "^/.+/events/[0-9]+",
        "^/events/[0-9]+",
        "^/.+/events/[a-z0-9-]+"
      ],
      "blockPaths": ["^/find/", "^/events/?$", "^/.+/events/?$"]
    },
    {
      "domain": "www.meetup.com",
      "allowPaths": [
        "^/.+/events/[0-9]+",
        "^/events/[0-9]+", 
        "^/.+/events/[a-z0-9-]+"
      ],
      "blockPaths": ["^/find/", "^/events/?$", "^/.+/events/?$"]
    }
  ]
}

